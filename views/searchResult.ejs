<%- include("./partial/header.ejs")%>
<%- include("./partial/navbar.ejs")%>

<div class="container-fluid listpage">
    <div class="searchResultBox">
        <h5 class="listHead">Search result for ❝ <%= key %> ❞</h5>

        <div class="searchResultListBox">
            <% for (let searchProject of searchProjects) { %>
            <% for (let list of searchProject.list) { %>
            <div class="listItem">
                <span class="checkBox <%= list.check ? "checked" : null %>" data-listid="<%= list._id %>"></span>
                <div class="listItemBox">
                    <a class="projectLink currentList <%= list.check ? "checkedText" : null %>"
                        href="/list/<%=searchProject.num%>">
                        <%= list.content %>
                    </a>
                </div>
            </div>
            <% } %>
            <% } %>
        </div>
    </div>
</div>
<script src="/static/js/list.js"></script>